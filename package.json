{
  "name": "style-props-html",
  "version": "0.0.4",
  "description": "A library that facilitates passing CSS styles as regular props using a collection of custom components that mirror traditional DOM elements.",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./utility-style-props": {
      "import": "./dist/esm/utility-style-props.js",
      "require": "./dist/cjs/utility-style-props.js",
      "types": "./dist/types/utility-style-props.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikeandike523/style-props-html.git"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "bootstrap": "./__python src/bootstrap.py"
  },
  "keywords": [
    "style-props",
    "html",
    "emotion",
    "react"
  ],
  "author": "Michael Sohnen",
  "license": "LICENSE",
  "devDependencies": {
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.13.5",
    "@types/lodash": "^4.17.13",
    "@types/react": "^19.0.1",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
